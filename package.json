{
  "name": "github-profile-summary",
  "author": "Grasp Mobile Development, LLC",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "concurrently": "^3.6.1",
    "node-sass-chokidar": "^1.3.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "node_modules/.bin/concurrently -n \"server,client\" -c \"yellow,cyan\" \"npm run server-start\" \"npm run client-start\"",
    "client-start": "react-scripts start",
    "server-start": "npm run start --prefix server",
    "server-build": "npm run build --prefix server",
    "server-dev": "npm run dev --prefix server",
    "server-test": "npm run test --prefix server",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive"
  }
}
